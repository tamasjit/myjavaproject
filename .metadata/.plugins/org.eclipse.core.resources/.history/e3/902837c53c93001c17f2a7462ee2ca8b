package assignment1;

public class P {
	public static void main(String args[]) {
		new M().methodPublic();
		new M().methodProtected();
		new M().methodDefault(); 
		//System.out.println("Value of long"+ new M().y);
		System.out.println("Value of long"+ new M().k);
		System.out.println("Value of long"+ new M().z);
		new N().methodPublic();
		new N().methodProtected();
		new N().methodDefault(); 
		//System.out.println("Value of long"+ new N().y);
		System.out.println("Value of long"+ new N().k);
		System.out.println("Value of long"+ new N().z);
	}

	public void methodPublic() {
		System.out.println("Class : methodPublic");
	}

	protected void methodProtected() {
		System.out.println("Class TestB: methodProtected");
		
	}

	void methodDefault() {
		System.out.println("Class TestB: methodDefault");
	}

	private void methodPrivate() {
		System.out.println("Class TestB: methodPrivate");
	}

}

class M {

	private int y= 10;   //private variable
	long k = 1000;       // default variable
	protected float z= 33;
	
	public void methodPublic(){     // public method
		methodPrivate();
	}
	
	protected void methodProtected(){   // protected method
		methodPrivate();
	}
	
	void methodDefault(){    //default method
		methodPrivate();
	}
	
	private void methodPrivate(){     // private method
		System.out.println("Class M: methodProtected");
		System.out.println("Value of private int y"+ y);
		System.out.println("Value of long k"+ k);
		System.out.println("Value of float z"+ z);
	}
}
class N {

	protected long y = 10;   //private variable
	
	public int k = 1000;       // default variable
	double z= 33;
	
	public void methodPublic(){     // public method
		methodPrivate();
	}
	
	protected void methodProtected(){   // protected method
		methodPrivate();
	}
	
	void methodDefault(){    //default method
		methodPrivate();
	}
	
	private void methodPrivate(){     // private method
		System.out.println("Class N: methodProtected");
		System.out.println("Value of long y"+ y);
		System.out.println("Value of int"+ k);
		System.out.println("Value of double"+ z);
	}
}
class X{
	private  int aa= 159;
	long bb = 254;
	protected float pf = 543.33f;
	public char d = 'X';
	public void methodPublic(){     // public method
		methodPrivate();
	}
	private void methodPrivate() {
		System.out.println("Value of long aa"+ aa);
	}
}
public class Y {
	public static void main(String args[]) {
		new M().methodPublic();
		new M().methodProtected();
		new M().methodDefault();
		new N().methodPublic();
		new N().methodProtected();
		new N().methodDefault();
		new X().methodPublic(); 
		System.out.println("Value of long"+ new X().bb);
		System.out.println("Value of long"+ new X().pf);
		System.out.println("Value of long"+ new X().d);
		
	}
}

